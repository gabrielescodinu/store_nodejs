<!DOCTYPE html>
<html>
  <head>
    <title>Students List</title>
  </head>
  <body>
    <h1>Students List</h1>
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th>Email</th>
          <th>Message</th>
        </tr>
      </thead>
      <tbody>
        <% students.forEach(student => { %>
          <tr>
            <td><%= student.name %></td>
            <td><%= student.email %></td>
            <td><%= student.message %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
